.inpt_button input {
	display: none !important;
}
.inpt_button label {
	display: inline-block;
	width: 20%;
	text-align: right;
	padding-right: 5%;
	color: #999999;
}

.inpt_button label {
	width: 15%;
	height: 30px;
	line-height: 30px;
	background: #3B9CFF;
	color: white;
	border-radius: 15px;
	text-align: center;
	padding-right: 0px;
	font-size: 14px;
	float: right;
	margin-top: 10px;
}

.file_list_name {
	float: left;
}

.file_list_delete {
	color: #DD524D;
}