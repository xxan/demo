
.header_box .mui-nav-more {
	display: inline-block;
	width: 40px;
	height: 100%;
	line-height: 44px;
	float: right;
	position: relative;
	right: 0px;
}

.header_box .mui-nav-more img{
	position: relative;
	top: 0px;
	right: -5px;
}

.mui-nav-more_nr {
	width: 120px;
	height: 82px;
	background: url(../img/know_box.png) no-repeat;
	background-size: 100% 100%;
}
.mui-nav-more_nr {
	padding: 0px 10px;
}
.mui-nav-more_nr p {
	height: 40px;
	margin: 0;
	border-bottom: solid 1px #F2F2F2;
}

.mui-nav-more_nr p span {
	font-size: 12px;
}

.mui-nav-more_nr p:last-child {
	border-bottom: none;
	line-height: 20px;
	height: 30px;
}

.mui-nav-more_nr p:last-child img {
	top: 10px;
}

.mui-nav-more_nr p:last-child span {
	top: 5px;
}

.know_title {
	height: 35px;
	background: white;
	line-height: 35px;
	padding-left: 20px;
}

.know_title span {
	display: inline-block;
	height: 100%;
	font-size: 14px;
	color: #666666;
	position: relative;
	text-align: center;
	margin-right: 10px;
}

