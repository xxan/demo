			var year = 0;
			var innerHtml = '';
			for (var i = 0; i < res.data.length; i++) {
				var item = res.data[i];
				var visitTime = item.VisitTime;
				if (year != visitTime.split('-')[0]) {
					year = visitTime.split('-')[0];
					console.log('year:', year);
					if (item.State == 0) {
						innerHtml += '<li class="end_save" listId="' + item.Id +
