.know_title span.active {
	color: #3B9CFF;
	font-weight: bold;
}

.know_title span.active i {
	display: inline-block;
	width: 30px;
	height: 2px;
	background: #3B9CFF;
	border-radius:1px;
	position: absolute;
	bottom: 3px;
	left: 15px;
}

.up_fress {
	top: 0px;
	background: #F2F2F2;
	/* opacity: 0; */
	display: none;
}
.down_fress {
	background: #F2F2F2;
}

#pullrefresh {
	top: 85px;
	background: #F2F2F2;
}

.revene_all {
	background: white;
	margin: 0;
	border-top: solid 1px #CCCCCC;
}

.revene_all li {
	border-bottom: solid 1px #CCCCCC;
	height: 44px;
	position: relative;
}

.revene_all li .list_info {
	padding-left: 20px;
	height: 43px;
	line-height: 44px;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 99;
	width: 100%;
	background: white;
}

.revene_all li .list_info img {
	/* vertical-align: middle; */
	width: 20px;
	position: relative;
	top: -15px;
	margin-right: 5px;
}

.revene_all li .list_info span {
	color: #333333;
	font-size: 14px;
	width: 80%;
	display: inline-block;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.list_btn {
	width: 120px;
	height: 43px;
	position: absolute;
	right: 0;
	top: 0;
	background: #14804E;
	z-index: 0;
}

.list_btn span {
	width: 50%;
	height: 100%;
	display: inline-block;
	text-align: center;
}

.list_btn span.list_btn_edit {
	background: url(../img/know_edit.png) no-repeat center;
	background-size: 50%;
	background-color: #6091FF;
}

.list_btn span.list_btn_del {
	background: url(../img/know_del.png) no-repeat center;
	background-size: 50%;
	background-color: #FF6060;
}

.list_btn span img {
	width: 40%;
	position: relative;
	top: 10px;
}

.add_file_modal {
	display: none;
}

.add_file_box {
	width: 60%;
	max-width: 250px;
	height: 135px;
	background: white;
	border-radius: 10px;
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	margin: auto;
	text-align: center;
}

.add_file_box p {
	width: 100%;
	text-align: center;
	font-weight: 400;
	color: #333333;
	margin-top: 20px;
	font-size: 14px;
}

.add_file_box input {
	width: 90%;
	padding: 0px 10px;
	border: none;
	font-size: 12px;
	background: #F7F7F7;
	height: 30px;
}

.add_file_box .btn_modal {
	width: 100%;
	height: 35px;
	border-top: solid 1px #E6E6E6;
}

.btn_modal span {
	display: inline-block;
	width: 50%;
	height: 100%;
	line-height: 35px;
	font-size: 14px;
	color: #3B9CFF;
}


.task_details p {
	background: white;
	padding: 0px 20px;
	height: 48px;
	line-height: 46px;
	border-bottom: solid 1px #E6E6E6;
	font-size: 0.75rem;
	margin: 0;
}

.task_details p span {
	display: inline-block;
	width: 30%;
	color: #333333;
}

.task_details p span i {
	color: #DD524D;
}

.task_details p input {
	display: inline-block;
	width: 70%;
	height: 100%;
	border: none;
	text-align: right;
	padding: 0;
	margin: 0;
	float: right;
	font-size: 12px;
	color: #666666;
}

.file_list {
	width: 100%;
	padding: 0px 20px;
	margin: 0;
	background: white;
}

.file_list li {
	height: 30px;
	line-height: 30px;
	font-size: 12px;
	text-align: right;
	border-bottom: dotted 1px #F2F2F2;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.file_list li a {
	width: 85%;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	text-align: left;
}

.inpt_button input {
	display: none !important;
}
.inpt_button label {
	display: inline-block;
	width: 20%;
	text-align: right;
	padding-right: 5%;
	color: #999999;
}

.inpt_button label {
	width: 15%;
	height: 30px;
	line-height: 30px;
	background: #3B9CFF;
	color: white;
	border-radius: 15px;
	text-align: center;
	padding-right: 0px;
	font-size: 14px;
	float: right;
	margin-top: 10px;
}

.file_list_name {
	float: left;
}

.file_list_delete {
	color: #DD524D;
}
.list_box {
	margin: 0;
}
.list_box li {
	height: 65px;
	border-bottom: solid 1px #CCCCCC;
	padding-left: 20px;
	background: white;
	position: relative;
}

.list_box li .list_info {
	width: 100%;
	height: 64px;
	line-height: 64px;
	position: relative;
	z-index: 1;
	background: white;
}

.list_box li .list_info .img_box {
	width: 40px;
	height: 100%;
	float: left;
}

.list_box li .list_info .img_box img {
	width: 28px;
	position: relative;
	top: 6px;
}

.list_box li .list_info .text_box {
	float: left;
	width: calc(100% - 40px);
}

.list_box li .list_info .text_box p {
	height: 18px;
	line-height: 18px;
	margin: 0;
	margin-top: 10px;
	font-size: 14px;
	color: #333333;
	width: 100%;
	padding-right: 20px;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.list_box li .list_info .text_box p:last-child {
	margin-top: 5px;
	color: #666666;
	font-size: 12px;
}

.list_box li .list_info .text_box p:last-child span {
	display: inline;
	font-size: 12px;
	color: #666666;
	float: right;
	margin-right: 20px;
}

.list_box li .list_btn {
	height: 64px;
}

.list_box li .list_btn img {
	top: 20px;
}

.bid_no_data {
	display: none;
}

.project_name {
	height: 44px;
	background: white;
	padding: 0px 20px;
	line-height: 44px;
	margin: 0;
	color: #333333;
	font-size: 16px;
}

#pullrefresh h5 {
	padding-left: 20px;
	font-size: 12px;
	color: #999999;
}

.add_btn {
	width: 100%;
	height: 40px;
	line-height: 40px;
	text-align: center;
}

.add_btn input {
	display: none;
}

.add_btn span, .add_btn label{
	width: 140px;
	height: 30px;
	line-height: 30px;
	background: #3B9CFF;
	color: white;
	display: inline-block;
	border-radius: 5px;
	font-size: 12px;
}
