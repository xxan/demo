.meau_list {
				width: 100%;
				height: 100%;
				position: fixed;
				top: 0;
				background: rgba(0,0,0,0.5);
				right: -100%;
				z-index: 999;
			}
			.meau_list .list_table {
				width: 80%;
				height: 100%;
				background: white;
				position: absolute;
				right: 0;
				padding-top: 44px;
			}
			
			.meau_list .list_table p {
				height: 50px;
				line-height: 50px;
				padding-left: 20px;
				margin: 0;
			}
			.meau_list .list_table .items {
				overflow: hidden;
				padding: 10px;
			}
			.meau_list .list_table .items span{
				display: inline-block;
				padding: 0px 10px;
				height: 30px;
				line-height: 30px;
				text-align: center;
				background: #F2F2F2;
				float: left;
				margin-right: 20px;
				margin-bottom: 10px;
				font-size: 14px;
			}
			
			.meau_list .list_table .items span.active {
				background: #3B9CFF;
				color: white;
			}
			
			.meau_list .list_table p>button {
				width: 30%;
				margin-right: 20px;
			}
			.meau_list .list_table .btn_box_meau {
				margin-top: 30px;
			}